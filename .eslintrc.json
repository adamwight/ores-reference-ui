{
  "extends": "wikimedia",
  "plugins": [
    "react"
  ],
  "globals": {
	"document": true,
    "fetch": true,
    "module": false,
    "URL": true,
    "URLSearchParams": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
     },
    "sourceType": "module",
    "ecmaVersion": "6"
  },
  "rules": {
    "react/jsx-uses-vars": 1,
    "react/jsx-uses-react": 1
  }
}
